{
  "scheduler": {
    "max_concurrent_dags": 10,
    "check_interval": 60,
    "enable_catchup": true,
    "timezone": "UTC"
  },
  "executor": {
    "type": "local",
    "max_workers": 4,
    "timeout": 300
  },
  "database": {
    "connections": {
      "postgres_default": {
        "host": "localhost",
        "port": 5432,
        "database": "airflow",
        "username": "airflow",
        "password": "airflow"
      },
      "mysql_default": {
        "host": "localhost",
        "port": 3306,
        "database": "airflow",
        "username": "airflow",
        "password": "airflow"
      }
    }
  },
  "logging": {
    "level": "INFO",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
  },
  "webserver": {
    "port": 8080,
    "host": "0.0.0.0",
    "secret_key": "your-secret-key-here"
  }
}
